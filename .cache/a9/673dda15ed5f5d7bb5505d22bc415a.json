dex.scss","name":null,"original":{"line":223,"column":4},"generated":{"line":1,"column":2945}},{"source":"styles\\index.scss","name":null,"original":{"line":224,"column":4},"generated":{"line":1,"column":2946}},{"source":"styles\\index.scss","name":null,"original":{"line":224,"column":4},"generated":{"line":1,"column":2954}},{"source":"styles\\index.scss","name":null,"original":{"line":225,"column":4},"generated":{"line":1,"column":2955}},{"source":"styles\\index.scss","name":null,"original":{"line":227,"column":2},"generated":{"line":1,"column":2965}},{"source":"styles\\index.scss","name":null,"original":{"line":228,"column":4},"generated":{"line":1,"column":2983}}],"sources":{"styles\\index.scss":"$primary-color: orange;\r\n\r\nh1 {\r\n    color: $primary-color; \r\n}\r\n/*root: Bootswatch - cosmo */\r\n:root {\r\n  --blue: #2780e3;\r\n  --indigo: #6610f2;\r\n  --purple: #613d7c;\r\n  --pink: #e83e8c;\r\n  --red: #ff0039;\r\n  --orange: #f0ad4e;\r\n  --yellow: #ff7518;\r\n  --green: #3fb618;\r\n  --teal: #20c997;\r\n  --cyan: #9954bb;\r\n  --white: #fff;\r\n  --gray: #868e96;\r\n  --gray-dark: #373a3c;\r\n  --primary: #2780e3;\r\n  --secondary: #373a3c;\r\n  --success: #3fb618;\r\n  --info: #9954bb;\r\n  --warning: #ff7518;\r\n  --danger: #ff0039;\r\n  --light: #f8f9fa;\r\n  --dark: #373a3c;\r\n}\r\n\r\n/* Elements */\r\n  html {\r\n    font-size: 8px;\r\n  }\r\n  body {\r\n    font-family: Verdana, Tahoma, sans-serif, Helvetica, sans-serif;\r\n    overflow: hidden;\r\n    background: black;\r\n}\r\n\r\n.btn:focus {\r\n  outline: none;\r\n  -webkit-box-shadow: none;\r\n  box-shadow: none;\r\n}\r\n.fitContent {\r\n  width: fit-content !important;\r\n}\r\n.modal-xl {\r\n  max-width: 70%;\r\n}\r\n\r\n/* Floater classes */\r\n  .isSelected {\r\n    border: 3px solid lime !important;\r\n  }\r\n  .floater {\r\n    position: absolute;\r\n    box-sizing: border-box;\r\n    resize: both;\r\n    overflow: hidden;\r\n    border: 1px solid slategray;\r\n    border-radius: 0.5rem;\r\n}\r\n\r\n/* Font & text-size classes */\r\n  .smallCaps {\r\n    font-variant: small-caps;\r\n  }\r\n  .spaceWide {\r\n    letter-spacing: 3px;\r\n  }\r\n  .text120 {\r\n    font-size: 120%;\r\n  }\r\n  .text140 {\r\n    font-size: 140%;\r\n  }\r\n  .text160 {\r\n    font-size: 160%;\r\n  }\r\n  .text180 {\r\n    font-size: 180%;\r\n  }\r\n  .text200 {\r\n    font-size: 200%;\r\n  }\r\n  .text220 {\r\n    font-size: 220%;\r\n  }\r\n  .text240 {\r\n    font-size: 240%;\r\n}\r\n\r\n/* Tooltip classes */\r\n  .tooltip.show {\r\n    opacity: 1;\r\n  }\r\n  .tooltip-inner {\r\n    max-width: unset;\r\n    padding: 0;\r\n    text-align: left;\r\n    background-color: white;\r\n    color: black;\r\n    border: 1px solid #cd0200;\r\n  }\r\n  .tooltip.bs-tooltip-right .arrow:before {\r\n    border-right-color: #cd0200 !important;\r\n  }\r\n  .tooltip.bs-tooltip-left .arrow:before {\r\n    border-left-color: #cd0200 !important;\r\n  }\r\n  .tooltip.bs-tooltip-bottom .arrow:before {\r\n    border-bottom-color: #cd0200 !important;\r\n  }\r\n  .tooltip.bs-tooltip-top .arrow:before {\r\n    border-top-color: #cd0200 !important;\r\n}\r\n\r\n/* Misc id's */\r\n  #collageWrapper {\r\n    opacity: 0.01;\r\n    height: 100vh;\r\n  }\r\n  #loadingGif {\r\n    display: none;\r\n    position: absolute;\r\n    top: 50%;\r\n    left: 50%;\r\n    -ms-transform: translate(-50%, -50%);\r\n    transform: translate(-50%, -50%);\r\n    z-index: 1000;\r\n  }\r\n  #displayOrderBtn {\r\n    display: none;\r\n    position: absolute;\r\n    top: 1rem;\r\n    left: 50%;\r\n    -ms-transform: translateX(-50%);\r\n    transform: translateX(-50%);\r\n    z-index: 1000;\r\n  }\r\n  #navdropdown:hover {\r\n    text-decoration: none;\r\n  }\r\n  #offscreenImgLoader {\r\n    position: absolute;\r\n    top: 180vh;\r\n}\r\n\r\n/* Help Modal */\r\n  #helpModal {\r\n    font-size: 280% !important;\r\n    font-weight: 900 !important;\r\n    letter-spacing: 1px;\r\n  }\r\n  #helpModal * {\r\n    color: black;\r\n  }\r\n  #helpModal .nav-tabs {\r\n    border-bottom: 2px solid var(--danger);\r\n  }\r\n  #helpModal .nav-tabs .nav-item {\r\n    margin-bottom: -2px !important;\r\n  }\r\n  #helpModal .nav-tabs .nav-link.active {\r\n    border-bottom: none;\r\n    border-color: var(--danger);\r\n    border-width: 2px;\r\n  }\r\n  #helpModal .nav-tabs .nav-link:not(.active):hover {\r\n    color: var(--success) !important;\r\n    border: none !important;\r\n  }\r\n  #helpModal .tab-content>.tab-pane.active {\r\n    border: 2px solid var(--danger);\r\n    border-top: none;\r\n  }\r\n  #quickstart div > p:last-child {\r\n    background-color: yellow;\r\n  }\r\n  \r\n  #howtoedit > div:not(:first-child) div p:last-child,\r\n  #howtoedit > div.w-50.mt-4.mx-auto.text-center > p:last-child {\r\n    background-color: yellow;\r\n  }\r\n  // #howtoedit > div:not(:first-child) div p:last-child {\r\n  //   padding: 0.75rem;\r\n  // }\r\n\r\n/* Loading Dock */\r\n  .loadingDock_init {\r\n    position: absolute;\r\n    display: flex;\r\n    flex-direction: column;\r\n    width: 66.6vw;\r\n    height: 66.6vh;\r\n    margin: 0;\r\n    top: 18vh;\r\n    left: 16.65vw;\r\n    justify-content: center;\r\n    background-color: black !important;\r\n    border-width: 0.25rem !important;\r\n    border-style: ridge;\r\n    z-index: 1;\r\n  }\r\n  .loadingDock_transition {\r\n    position: absolute;\r\n    display: flex;\r\n    width: 100vw;\r\n    height: 100vh;\r\n    margin: 0;\r\n    top: 0;\r\n    left: 0;\r\n    transition: top 2s, left 2s, width 2s, height 2s;\r\n    justify-content: center;\r\n    background-color: black !important;\r\n    border-width: 0.25rem !important;\r\n    border-style: ridge;\r\n  }\r\n  #selectImagesBtn {\r\n    /* File uploader - hide ugly <input> element */\r\n    width: 0;\r\n    height: 0;\r\n    opacity: 0;\r\n  }\r\n  #createCollageBtn {\r\n    display: none;\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"c471bbf8e8ce801a61894365ced04ffe","cacheData":{"env":{}}}